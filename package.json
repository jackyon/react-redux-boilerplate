{
  "name": "webpack-structure",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tinypng": "tinypng -r dist/",
    "build-dll": "webpack --config webpack.dll.config.js && cp -R build/vendor-manifest.json build/vendor.dll.js dist/;",
    "browsersyncDevCommon": "mv build/vendor-manifest.json build/vendor.dll.js ./; rm -rf build/*; mv vendor-manifest.json vendor.dll.js build/; cp -R app/public/* build/;",

    "browsersync": "npm run browsersyncDevCommon; webpack --watch",
    "dev": "npm run browsersyncDevCommon; webpack-dev-server --host 0.0.0.0 --devtool eval --progress --colors --inline --hot --content-base build --congfig webpack.config.js; ",
    "dev-quiet": "npm run dev | grep -v '[[0-9]]'",
    "deploy": "cp -R app/public/* dist/; webpack",
    "lint": "eslint --fix --format 'node_modules/eslint-friendly-formatter' . --ext .js --ext .jsx || true",
    "stats": "webpack --profile --json > stats.json"
  },
  "author": "",
  "license": "ISC",
